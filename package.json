{
  "name": "cloudflare-pages-cleanup",
  "version": "1.1.0",
  "description": "GitHub Action to cleanup Cloudflare Pages deployments",
  "private": true,
  "main": "lib/index.js",
  "scripts": {
    "all": "pnpm run check && pnpm run test && pnpm run build && pnpm run package",
    "build": "tsc",
    "check": "pnpm run fmt:check && pnpm run lint:check",
    "fmt:fix": "prettier --write .",
    "fmt:check": "prettier --check .",
    "lint:fix": "eslint --fix",
    "lint:check": "eslint",
    "package": "ncc build --license licenses.txt --minify",
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mfinelli/cloudflare-pages-cleanup.git"
  },
  "keywords": [
    "cloudflare"
  ],
  "author": "Mario Finelli",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mfinelli/cloudflare-pages-cleanup/issues"
  },
  "homepage": "https://github.com/mfinelli/cloudflare-pages-cleanup#readme",
  "dependencies": {
    "@actions/artifact": "^2.3.2",
    "@actions/core": "^1.11.1",
    "cloudflare": "^5.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.7.2",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vitest": "^3.2.4"
  }
}
